{
  "name": "ProjectView",
  "version": "0.2.0",
  "description": "A simple and clean manga, gallery viewer for macOS.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "build --dir",
    "dist": "build",
    "collect-renders": "cp /Users/shixiao/go/src/renders/renders ./",
    "collect-app": "cp -r ../viewer/dist ./app"
  },
  "keywords": [],
  "author": "Centimitr",
  "devDependencies": {
    "electron": "^1.6.3",
    "electron-builder": "next"
  },
  "build": {
    "appId": "com.devbycm.projectviewer",
    "productName": "Project View",
    "mac": {
      "category": "your.app.category.type"
    },
    "compression": "store",
    "asarUnpack": [
      "1"
    ],
    "fileAssociations": {
      "ext": [
        "jpg",
        "png",
        "gif"
      ]
    }
  }
}